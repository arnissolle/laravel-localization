{
    "name": "arnissolle/laravel-localization",
    "description": "Localization middleware for Laravel 5.8 and up",
    "homepage": "https://github.com/arnissolle/laravel-localization",
    "keywords": [
        "arnissolle",
        "laravel",
        "localization",
        "middleware"
    ],
    "minimum-stability": "stable",
    "license": "MIT",
    "authors": [
        {
            "name": "Pierre Arnissolle",
            "email": "pierre@arnissolle.me",
            "homepage": "https://pierrearnissolle.me",
            "role": "Developer"
        }
    ],
    "require": {
        "laravel/framework": "^5.8"
    },
    "require-dev": {
        "phpunit/phpunit": "^8.1"
    },
    "autoload": {
        "psr-4": {
            "Arnissolle\\Localization\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Arnissolle\\Localization\\Test\\": "tests"
        }
    },
    "scripts": {
        "test": "phpunit"
    },
    "config": {
        "sort-packages": true
    },
    "extra": {
        "laravel": {
            "providers": [
                "Arnissolle\\Localization\\ServiceProvider"
            ]
        }
    }
}
