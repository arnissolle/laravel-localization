{
    "name": "arnissolle/laravel-localization",
    "description": "Laravel localization package",
    "keywords": [
        "arnissolle",
        "laravel",
        "localization",
        "middleware",
        "language",
        "l10n"
    ],
    "homepage": "https://github.com/arnissolle/laravel-localization",
    "license": "MIT",
    "type": "library",
    "authors": [
        {
            "name": "Pierre Arnissolle",
            "email": "pierre@arnissolle.com",
            "homepage": "https://pierrearnissolle.com",
            "role": "Developer"
        }
    ],
    "require": {
        "php": "^7.2",
        "laravel/framework": "^6.0"
    },
    "require-dev": {
        "orchestra/testbench": "^4.0",
        "phpunit/phpunit": "^8.0"
    },
    "autoload": {
        "psr-4": {
            "Arnissolle\\Localization\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Arnissolle\\Localization\\Tests\\": "tests"
        }
    },
    "scripts": {
        "test": "vendor/bin/phpunit",
        "test-coverage": "vendor/bin/phpunit --coverage-html coverage"

    },
    "config": {
        "sort-packages": true
    },
    "extra": {
        "laravel": {
            "providers": [
                "Arnissolle\\Localization\\LocalizationServiceProvider"
            ],
            "aliases": {
                "Localization": "Arnissolle\\Localization\\LocalizationFacade"
            }
        }
    }
}
