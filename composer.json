{
    "name": "arnissolle/laravel-localization",
    "description": "Laravel localization package",
    "homepage": "https://github.com/arnissolle/laravel-localization",
    "keywords": [
        "arnissolle",
        "laravel",
        "localization",
        "middleware",
        "language",
        "i18n"
    ],
    "license": "MIT",
    "authors": [
        {
            "name": "Pierre Arnissolle",
            "email": "pierre@arnissolle.me",
            "homepage": "https://pierrearnissolle.me",
            "role": "Developer"
        }
    ],
    "require": {
        "php" : ">=7.1.0",
        "laravel/framework": "^5.8.0"
    },
    "require-dev": {
        "phpunit/phpunit": "^8.1"
    },
    "autoload": {
        "psr-4": {
            "Arnissolle\\Localization\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Arnissolle\\Localization\\Test\\": "tests"
        }
    },
    "scripts": {
        "test": "phpunit"
    },
    "extra": {
        "laravel": {
            "providers": [
                "Arnissolle\\Localization\\Support\\ServiceProvider"
            ],
            "aliases": {
                "Localization": "Arnissolle\\Localization\\Facades\\Localization"
            }
        }
    },
    "config": {
        "sort-packages": true
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
